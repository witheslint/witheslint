{
  "name": "@witheslint/core",
  "version": "0.9.0",
  "type": "module",
  "description": "Implementing a standardized ESLint configuration across all your projects.",
  "author": "Joie <leaps@live.com>",
  "license": "MIT",
  "homepage": "https://github.com/witheslint/witheslint",
  "bugs": "https://github.com/witheslint/witheslint/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/witheslint/witheslint.git",
    "directory": "packages/core"
  },
  "keywords": [
    "eslint-config",
    "eslint-flat-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./modules": {
      "import": "./dist/modules.mjs",
      "types": "./dist/modules.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dts"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "unbuild",
    "dts": "esno ./scripts/generate-dts.ts && eslint ./dts --fix"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.6.0",
    "@types/eslint": "^8.56.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.6",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^13.24.0"
  },
  "peerDependencies": {
    "eslint": ">=8.56.0"
  }
}
