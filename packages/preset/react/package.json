{
  "name": "@witheslint/preset-react",
  "version": "0.1.0",
  "type": "module",
  "description": "ESLint config preset for react.",
  "author": "Joie <leaps@live.com>",
  "license": "MIT",
  "homepage": "https://github.com/witheslint/witheslint",
  "bugs": "https://github.com/witheslint/witheslint/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/witheslint/witheslint.git",
    "directory": "packages/preset/react"
  },
  "keywords": [
    "eslint-config",
    "eslint-flat-config",
    "eslint-preset",
    "react"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./modules": {
      "import": "./dist/modules.mjs",
      "types": "./dist/modules.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@witheslint/core": "workspace:^"
  },
  "peerDependencies": {
    "@witheslint/core": "workspace:^"
  }
}
